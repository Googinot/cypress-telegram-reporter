{
    "compilerOptions": {
        "outDir": "./.tsbuild/",
        "sourceMap": false,
        "target": "es2021",
        "module": "commonjs",
        "removeComments": true,
        "noImplicitAny": false,
        "strictPropertyInitialization": true,
        "strictNullChecks": true,
        "types": ["node"]
    },
    "include": [
      "./**/*.ts"
    ]
}
